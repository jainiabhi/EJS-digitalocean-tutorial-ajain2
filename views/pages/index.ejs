<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('C:\\Users\\Abhi\\OneDrive\\Desktop\\ejs-demo\\views\\partials\\head.ejs'); %>
</head>
<body class="container">

<header>
  <%- include('C:\\Users\\Abhi\\OneDrive\\Desktop\\ejs-demo\\views\\partials\\header.ejs'); %>
</header>

<main>
  <div class="jumbotron">
    <h1>Top mascots</h1>
    <p>Welcome to templating using EJS</p>
    <h2>Variable</h2>
    <p><%= tagline %></p>
    <ul>
      <% mascots.forEach(function(mascot) { %>
        <li>
          <strong><%= mascot.name %></strong>
          representing <%= mascot.organization %>,
          born <%= mascot.birth_year %>
        </li>
      <% }); %>
    </ul>
  </div>
</main>

<footer>
  <%- include('C:\\Users\\Abhi\\OneDrive\\Desktop\\ejs-demo\\views\\partials\\footer.ejs'); %>
</footer>

</body>
</html>